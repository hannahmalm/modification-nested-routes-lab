<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %>
  <!--<%= f.text_field :artist_name %>-->
  <!--Use the helper methods here -->
  <!--Displays the song name-->
  <%= display_name(@song, @path)%>
  <!--This helper displays a dropdown of artists if you want to edit a specific artist-->
  <%= artist_select(@song, @path)%><br />

  <%= f.submit %>
<% end %>
